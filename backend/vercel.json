{
  "version": 2,
  "builds": [
    {
      "src": "public/index.php",
      "use": "vercel-php@0.7.3"
    }
  ],
  "routes": [
    {
      "src": "/(css|js|storage)/(.+)",
      "dest": "public/$1/$2"
    },
    {
      "src": "/storage/(.+)",
      "dest": "storage/app/public/$1"
    },
    {
      "src": "/(.*)",
      "dest": "public/index.php"
    }
  ],
  "env": {
    "APP_ENV": "production",
    "APP_DEBUG": "false",
    "APP_URL": "https://your-backend-url.vercel.app",
    "LOG_CHANNEL": "stderr",
    "SESSION_DRIVER": "cookie",
    "CACHE_DRIVER": "array",
    "QUEUE_CONNECTION": "sync"
  }
}
